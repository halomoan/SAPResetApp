{
	"_version": "1.5.0",
	"sap.app": {
		"id": "app.sap.resetSAPResetApp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"default": {
				"uri": "https://services.odata.org/TripPinRESTierService/(S(id))/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"ZSAPRESET_SRV": {
				"uri": "/sap/opu/odata/sap/ZSAPRESET_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZSAPRESET_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "app.sap.resetSAPResetApp.view.App",
			"type": "XML",
			"async": true,
        	"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "app.sap.resetSAPResetApp.i18n.i18n"
				}
			},
			"": {
				"dataSource": "ZSAPRESET_SRV",
				"settings": {
					"useBatch": false
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		
      "routing": {
         "config": {
            "routerClass": "sap.m.routing.Router",
            "viewType": "XML",
            "viewPath": "app.sap.resetSAPResetApp.view",
            "controlId": "app",
            "controlAggregation": "pages",
            "transition": "slide",
            "async": true,
            "bypassed": {
               "target": "notFound"
            }
         },
         "routes": [{
            "pattern": "",
            "name": "resetSAP",
            "target": "resetSAP"
         },{
            "pattern": "resetLink/{resetcode}/{syscode}/source/{sourceid}",
            "name": "resetLink",
            "target": "resetLink"
         }],
         "targets": {
            "resetSAP": {
               "viewId": "resetSAP",
               "viewName": "resetSAP",
               "viewLevel" : 1
            },
            "resetLink": {
               "viewId": "resetLink",
               "viewName": "resetLink",
               "viewLevel" : 1
            },
            "notFound": {
               "viewId": "notFound",
               "viewName": "NotFound",
               "transition": "show"
            }
            
         }
      }
	}
}